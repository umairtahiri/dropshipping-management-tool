{
  "name": "dropshipping-dashboard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^14 || ^16 || ^18"
  },
  "scripts": {
    "debug": "NODE_OPTIONS='--inspect' next dev --port 3030",
    "dev": "next dev --port 3030",
    "build": "next build",
    "postbuild": "next-sitemap",
    "translations": "formatjs extract './{components,pages}/**/*.{ts,tsx}' --out-file temp.json --flatten --id-interpolation-pattern '[sha512:contenthash:base64:6]' && formatjs compile 'temp.json' --out-file locale/en-US.json && rm ./temp.json",
    "analyze-build": "ANALYZE=true next build",
    "start": "next start",
    "test": "jest",
    "lint": "prettier --write . && next lint --fix",
    "generate": "dotenv -c -- graphql-codegen --config codegen.yml -w",
    "paths": "pathpida --ignorePath .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "3.5.10",
    "@babel/core": "^7.18.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@formatjs/swc-plugin": "^1.4.0",
    "@formatjs/ts-transformer": "^3.9.2",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.15",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "1.0.5",
    "@mui/icons-material": "^5.8.2",
    "@mui/lab": "5.0.0-alpha.87",
    "@mui/material": "5.8.2",
    "@mui/styled-engine-sc": "^5.8.0",
    "@mui/styles": "^5.8.3",
    "@mui/system": "^5.4.1",
    "@mui/x-date-pickers": "^5.0.4",
    "@reduxjs/toolkit": "^1.8.4",
    "@saleor/sdk": "^0.4.4",
    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.26.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/auth0-js": "^9.14.7",
    "@types/jest": "^29.1.1",
    "auth0-js": "^9.20.1",
    "axios": "^0.27.2",
    "chart.js": "^3.9.1",
    "clsx": "1.1.1",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.7",
    "delay": "^5.0.0",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.1.0",
    "editorjs-blocks-react-renderer": "^1.2.4",
    "formik": "^2.2.9",
    "graphql": "16.3.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jquery": "^3.6.0",
    "material-ui-popup-state": "^3.1.1",
    "moment": "^2.29.4",
    "next": "13.1.6",
    "next-router-mock": "^0.9.1",
    "next-seo": "^5.3.0",
    "next-sitemap": "^2.4.3",
    "next-usequerystate": "1.7.0",
    "nextjs-progressbar": "^0.0.14",
    "notistack": "^2.0.8",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.29.0",
    "react-icons": "^4.4.0",
    "react-intl": "^5.24.6",
    "react-is": "16.8.0",
    "react-redux": "^8.0.2",
    "react-slick": "^0.29.0",
    "react-use": "^17.3.2",
    "redux": "^4.2.0",
    "sass": "^1.55.0",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^4.6.1",
    "stripe": "^11.6.0",
    "styled-components": "^5.3.1",
    "swr": "^1.3.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.8.2",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@next/bundle-analyzer": "^12.1.4",
    "@svgr/webpack": "^6.2.1",
    "@types/qs": "^6.9.7",
    "@types/react": "17.0.43",
    "@types/react-slick": "^0.23.8",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "autoprefixer": "10.4.2",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "pathpida": "^0.18.1",
    "postcss": "8.4.23",
    "prettier": "2.6.2",
    "react-hook-form": "^7.29.0",
    "react-use": "^17.3.2",
    "tailwindcss": "3.0.23",
    "typescript": "4.6.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18",
        "graphql": "16"
      }
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,css,md,json}": "prettier --write"
  }
}
